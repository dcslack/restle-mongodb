{
  "name": "restle-mongodb",
  "version": "0.0.1",
  "description": "Restle MongoDB adapter.",
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:server",
    "clean": "rimraf dist",
    "prepublish": "npm run clean && npm run build",
    "build": "npm run build:lib && npm run build:test",
    "build:lib": "mkdirp dist/lib && babel lib --out-dir dist/lib",
    "build:test": "mkdirp dist/test && babel test --out-dir dist/test",
    "test:lint": "eslint lib test",
    "test:server": "npm run clean && npm run build && tape dist/test | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcslack/restle-mongodb.git"
  },
  "author": "Dylan Slack <dylanslack@gmail.com> (http://www.dylanslack.me)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dcslack/restle-mongodb/issues"
  },
  "homepage": "https://github.com/dcslack/restle-mongodb#readme",
  "files": [
    "dist/lib",
    "dist/test",
    "lib/",
    "test/"
  ],
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.1.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.2",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "i": "^0.3.3",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.42"
  }
}
